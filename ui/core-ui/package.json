{
  "name": "core-ui",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start:ng": "ng serve",
    "build:ng": "ng build",
    "watch:ng": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "wait": "wait-on http://localhost:4200",
    "wait-and-electron": "npm run wait && npm run electron",
    "start": "concurrently \"npm:start:ng\" \"npm:wait-and-electron\"",
    "electron": "cross-env ELECTRON_IS_DEV=1 electron .",
    "build": "npm run build:ng -- --configuration production && electron-builder",
    "build:electron": "npm run build:ng -- --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/material": "^19.2.17",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "electron": "^36.3.1",
    "hexagon-grid": "^2.1.0",
    "honeycomb-grid": "^4.1.5",
    "marked": "^15.0.12",
    "ngx-markdown": "^19.1.1",
    "rxjs": "~7.8.0",
    "three": "^0.165.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.7",
    "@angular-eslint/builder": "19.4.0",
    "@angular-eslint/eslint-plugin": "19.4.0",
    "@angular-eslint/eslint-plugin-template": "19.4.0",
    "@angular-eslint/schematics": "19.4.0",
    "@angular-eslint/template-parser": "19.4.0",
    "@angular/cli": "^19.1.7",
    "@angular/compiler-cli": "^19.1.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.12",
    "@types/three": "^0.165.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron-builder": "^26.0.12",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "8.57.0",
    "jasmine-core": "~5.5.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "wait-on": "^8.0.3"
  }
}
